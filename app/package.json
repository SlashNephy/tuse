{
  "name": "tuse",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/tuse",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dts": "dts-bundle-generator -o ../providers/plugin.d.ts lib/search/plugin.ts && prettier --write ../providers/plugin.d.ts",
    "start": "next start",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@mantine/core": "4.2.8",
    "@mantine/hooks": "4.2.8",
    "@mantine/next": "4.2.8",
    "@slashnephy/typescript-extension": "0.1.1",
    "date-fns": "2.28.0",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-query": "3.39.1",
    "tabler-icons-react": "1.48.1"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.1.6",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "17.0.31",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "concurrently": "7.2.1",
    "dts-bundle-generator": "6.11.0",
    "eslint": "8.17.0",
    "prettier": "2.6.2",
    "typescript": "4.7.3"
  },
  "packageManager": "yarn@3.2.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "rules": {
      "@typescript-eslint/no-unsafe-member-access": "off",
      "@typescript-eslint/no-unsafe-call": "off"
    }
  }
}
