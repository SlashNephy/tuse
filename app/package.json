{
  "name": "tuse",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/tuse",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dts": "dts-bundle-generator -o ../providers/plugin.d.ts lib/search/plugin.ts && prettier --write ../providers/plugin.d.ts",
    "start": "next start",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@mantine/core": "4.2.12",
    "@mantine/hooks": "4.2.12",
    "@mantine/next": "4.2.12",
    "@slashnephy/typescript-extension": "0.2.3",
    "date-fns": "2.29.3",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "tabler-icons-react": "1.55.0"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.2.7",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "17.0.31",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "concurrently": "7.4.0",
    "dts-bundle-generator": "7.0.0",
    "eslint": "8.26.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "18.0.21"
  },
  "packageManager": "yarn@3.2.2",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
