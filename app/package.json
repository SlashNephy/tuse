{
  "name": "tuse",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/tuse",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dts": "dts-bundle-generator -o ../providers/plugin.d.ts lib/search/plugin.ts && prettier --write ../providers/plugin.d.ts",
    "start": "next start",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@mantine/core": "4.2.12",
    "@mantine/hooks": "4.2.12",
    "@mantine/next": "4.2.12",
    "@slashnephy/typescript-extension": "0.2.3",
    "date-fns": "3.6.0",
    "next": "12.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "tabler-icons-react": "1.55.0"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "2.1.3",
    "@slashnephy/prettier-config": "1.0.73",
    "@types/node": "18.16.7",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "concurrently": "8.0.1",
    "dts-bundle-generator": "8.0.1",
    "eslint": "8.40.0",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@types/react": "18.2.6"
  },
  "packageManager": "yarn@3.5.0",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
