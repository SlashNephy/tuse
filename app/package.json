{
  "name": "tuse",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/tuse",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dts": "dts-bundle-generator -o ../providers/plugin.d.ts lib/search/plugin.ts && prettier --write ../providers/plugin.d.ts",
    "start": "next start",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@mantine/core": "4.2.11",
    "@mantine/hooks": "4.2.11",
    "@mantine/next": "4.2.11",
    "@slashnephy/typescript-extension": "0.2.0",
    "date-fns": "2.28.0",
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.1",
    "tabler-icons-react": "1.51.0"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.1.7",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "17.0.31",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "concurrently": "7.2.2",
    "dts-bundle-generator": "6.12.0",
    "eslint": "8.19.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
