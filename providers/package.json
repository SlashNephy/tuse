{
  "name": "tuse-search-providers",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/tuse",
  "scripts": {
    "build": "tsc --noEmit",
    "bundle": "TS_NODE_PROJECT=tsconfig.webpack.json webpack --color",
    "dev": "yarn bundle --watch",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@discordjs/rest": "0.5.0",
    "@octokit/rest": "18.12.0",
    "discord-api-types": "0.33.5",
    "node-fetch": "3.2.5"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.1.3",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "17.0.41",
    "@types/webpack": "5.28.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-remove-webpack": "^1.1.0",
    "concurrently": "7.2.1",
    "eslint": "8.17.0",
    "prettier": "2.6.2",
    "ts-loader": "9.3.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.3",
    "webpack": "5.73.0",
    "webpack-cli": "4.9.2"
  },
  "packageManager": "yarn@3.2.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "src/*/index.ts"
        ],
        "rules": {
          "import/no-default-export": "off"
        }
      }
    ]
  }
}
