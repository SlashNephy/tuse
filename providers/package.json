{
  "name": "tuse-search-providers",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/tuse",
  "scripts": {
    "build": "tsc --noEmit",
    "bundle": "TS_NODE_PROJECT=tsconfig.webpack.json webpack --color",
    "dev": "yarn bundle --watch",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@discordjs/rest": "0.6.0",
    "@octokit/rest": "19.0.5",
    "discord-api-types": "0.37.20",
    "node-fetch": "3.3.0",
    "tabler-icons-react": "1.55.0"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.2.35",
    "@slashnephy/prettier-config": "0.1.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/webpack": "5.28.0",
    "babel-loader": "9.1.2",
    "babel-plugin-dynamic-import-webpack": "1.1.0",
    "babel-plugin-remove-webpack": "1.1.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "concurrently": "7.6.0",
    "eslint": "8.31.0",
    "prettier": "2.8.2",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "packageManager": "yarn@3.3.0",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "src/*/index.{ts,tsx}"
        ],
        "rules": {
          "import/no-default-export": "off"
        }
      }
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
